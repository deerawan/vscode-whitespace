{
	"name": "vscode-whitespace",
	"displayName": "vscode-whitespace",
	"description": "",
	"version": "0.0.1",
	"publisher": "deerawan",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.convertTabsToSpaces",
        "onCommand:extension.convertSpacesToTabs"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
            {
			     "command": "extension.convertTabsToSpaces",
			     "title": "Convert Tabs to Spaces"
		    },
            {
			     "command": "extension.convertSpacesToTabs",
			     "title": "Convert Spaces to Tabs"
		    }
        ]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}